//Pepcoding

import java.util.*;
   
   public class Main{
   
   public static void main(String[] args) {
     // write your code here  
     Scanner sc = new Scanner(System.in);
     int num = sc.nextInt();
     while(num != 0)
     {
       int rem = num%10;
       System.out.println(rem);
       num = num/10;
     }
    }
   }
